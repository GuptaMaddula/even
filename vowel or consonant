import java.util.*;
class vowel
 {  
public static void main(String[] args)
{  
Scanner sc=new Scanner(System.in);
char ch = sc.next().charAt(0);   
switch(ch)
{  
case 'a': 
case 'b': 
case 'c':
case 'd':
case 'e': System.out.println("vowel");  
break;  
default:
System.out.println("consonant");  
}  
}  
}  
